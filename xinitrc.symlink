xrdb -merge ~/.Xresources
# export $(/usr/bin/gnome-keyring-daemon --start --components=pkcs11,secrets,ssh)
# export SSH_AUTH_SOCK
#xset b off
setxkbmap -option ctrl:nocaps

case $WM in
vm)
    exec spicey
    ;;
xfce4)
    exec startxfce4
	;;
i3)
    exec i3 -V >> ~/.i3-logs/i3log-$(date +'%F-%k-%M-%S').log 2>&1
	;;
*)
    exec i3 -V >> ~/.i3-logs/i3log-$(date +'%F-%k-%M-%S').log 2>&1
	;;
esac
